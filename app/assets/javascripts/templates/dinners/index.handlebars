<div class='row'>
  {{#each}}
    <div class='large-3 columns'>
      <div class='panel dinner-tile'>
        <h4 class='venue subheader'>{{venue}}</h4>
        <h4>{{format-date startsAt 'h:mm a'}}</h4>
        <p>{{format-date startsAt 'ddd, ll'}}</p>
        {{#link-to 'dinner' this class='tiny secondary button'}}Details{{/link-to}}
      </div>
    </div>
  {{/each}}
</div>


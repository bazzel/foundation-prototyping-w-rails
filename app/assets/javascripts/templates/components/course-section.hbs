<h3 class='subheader'>{{course.name}}</h3>
<div class="section-container vertical-tabs course" data-section="vertical-tabs">
{{#each course.dishes}}
  <section>
    <p class="title" data-section-title><a href="#">{{name}}</a></p>
    <div class="content" data-section-content>
      <ul>
        {{#each consumers}}
          <li>{{name}}</li>
        {{/each}}
      </ul>
    </div>
  </section>
{{/each}}
</div>
